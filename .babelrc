{
  "presets": [
      [ "@babel/preset-env",
          {
              "modules": false,
              "useBuiltIns": "usage"
          }
      ],
      "@babel/preset-react",
      "jest"
  ],
  "plugins": [
    ["react-css-modules", {
      "generateScopedName": "[name]__[local]___[hash:base64:5]",
    }]
  ],
  "env": {
      "test": {
          "presets": ["@babel/preset-env", "@babel/preset-react"],
          "plugins": ["transform-es2015-modules-commonjs"]
      }

  }
}
